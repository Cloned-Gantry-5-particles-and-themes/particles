{% extends '@nucleus/partials/particle.html.twig' %}

{% block particle %}
    <ul class="g-verticalmenu {{ particle.class|e }}">
        {% for item in particle.items %}
			<li>
            	<a target="{{ item.target }}" href="{{ item.link|e }}" title="{{ item.text }}">{{ item.text }}</a>
			</li>
        {% endfor %}
    </ul>
{% endblock %}